{
	"name" : "webpatser/laravel-uuid",
	"description" : "Laravel package to generate a UUID according to the RFC 4122 standard. Only support for version 1, 3, 4 and 5 UUID are built-in.",
	"type" : "laravel-library",
	"authors" : [{
			"name" : "Christoph Kempen",
			"email" : "christoph@downsized.nl"
		}
	],
	"keywords" : [
		"laravel uuid RFC4122"
	],
	"homepage" : "https://github.com/webpatser/laravel-uuid",
	"license" : [
		"MIT"
	],
	"require" : {
		"php" : ">=5.3.0"
	},
	"autoload" : {
		"psr-0" : {
			"Webpatser\\Uuid" : "src/"
		}
	},
	"minimum-stability" : "dev",
	"support" : {
		"source" : "https://github.com/webpatser/laravel-uuid",
		"issues" : "https://github.com/webpatser/laravel-uuid/issues"
	}
}